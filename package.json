{
  "name": "mousr",
  "productName": "Mousr",
  "version": "1.1.0",
  "description": "Move mouse cursor with keyboard",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "rebuild": "npm rebuild --runtime=electron --target=11.1.1 --disturl=https://atom.io/download/atom-shell --abi=72"
  },
  "keywords": [
    "cursor",
    "mouse",
    "keyboard",
    "mouser"
  ],
  "author": "darcy",
  "license": "ISC",
  "devDependencies": {
    "electron": "^11.4.11"
  },
  "dependencies": {
    "iohook": "^0.9.3",
    "robotjs": "^0.6.0"
  },
  "build": {
    "win": {
      "icon": "image/icon.png"
    }
  },
  "iohook": {
    "targets": [
      {
        "target": "electron",
        "abi": "85",
        "platform": "win32",
        "arch": "ia32"
      }
    ]
  }
}
